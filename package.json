    "build-production": "node buildjson.js && vue-cli-service build --mode production"
  },
  "dependencies": {
    "alasql": "^0.4.5",
    "axios": "^0.18.0",
    "cleave.js": "~1.1.1",
    "lodash": "^4.17.5",
    "moment": "~2.19.1",
    "pdfmake": "^0.1.36",
    "vue": "^2.5.16",
    "vue-i18n": "~7.3.2",
    "vue-print-nb": "^1.5.0",
    "vue-prodeb": "git+ssh://git@gitlab.prodeb.ba.gov.br:arquitetura/vue-prodeb.git#v1.0.3",
    "vue-router": "^3.0.1",
    "vue-the-mask": "~0.11.1",
    "vue2-filters": "~0.2.2",
    "vuetify": "~1.1.4",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "~2.4.2"
